<hibernate-mapping>
   <class name="datacollector.pojos.SystemPreferenceGroup" table="system_preference_group">
      <meta attribute="class-description">
         This class contains the system preference's group.
      </meta>
      <id name="id" type="int" column="system_preference_group_id">
         <generator class="native"/>
      </id>
      <property name="dateCreated" column="system_preference_group_date_created" type="date"/>
      <property name="lastModified" column="system_preference_group_last_modified" type="date"/>      
      <property name="name" column="system_preference_group_name" type="string"/>
   </class>
</hibernate-mapping>
