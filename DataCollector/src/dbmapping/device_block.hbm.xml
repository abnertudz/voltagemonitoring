<hibernate-mapping>
   <class name="datacollector.pojos.DeviceBlock" table="device_block">
      <meta attribute="class-description">
         This class contains the device block information.
      </meta>
      <id name="id" type="int" column="device_block_id">
         <generator class="native"/>
      </id>
      <property name="blockName" column="device_block_name" type="string"/>
      <property name="dateCreated" column="device_block_date_created" type="date"/>
      <property name="lastModified" column="device_block_last_modified" type="date"/>

   </class>
</hibernate-mapping>


